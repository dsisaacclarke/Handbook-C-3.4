#include <iostream>
#include <vector>
#include <set>

int main() {
    int n, k;
    std::cin >> n >> k;

    std::vector<int> arr(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> arr[i];
    }

    std::multiset<int> window;
    for (int i = 0; i < n; ++i) {
        window.insert(arr[i]);
        if (i >= k - 1) {
            std::cout << *window.begin() << "\n";
            window.erase(window.find(arr[i - k + 1]));
        }
    }
}

